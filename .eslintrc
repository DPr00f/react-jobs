{
  "root": true,
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "flowtype"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true,
  },
  "ecmaFeatures": {
    "defaultParams": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/sort-comp": 0
  }
}
